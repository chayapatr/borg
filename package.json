		"@lucide/svelte": "^0.534.0",
		"@sveltejs/adapter-cloudflare": "^7.1.2",
		"@xyflow/svelte": "^1.2.2",
		"firebase": "^12.0.0",
		"node-fetch": "^2.7.0",
		"partykit": "^0.0.111"
	},
